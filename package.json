  "scripts": {
    "prisma:migrate": "npx dotenv -e .env -- prisma migrate dev --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "npx dotenv -e .env -- prisma studio --schema apps/api/prisma/schema.prisma",
    "prisma:generate": "npx dotenv -e .env -- prisma generate --schema apps/api/prisma/schema.prisma",

    "nx:serve": "npx nx serve",
    "nx:build": "npx nx build",
    "nx:test": "npx nx test",
    "nx:lint": "npx nx lint",
    "nx:e2e": "npx nx e2e",
    "nx:affected:apps": "npx nx affected:apps",
    "nx:affected:libs": "npx nx affected:libs",
    "nx:affected:build": "npx nx affected:build",
    "nx:affected:e2e": "npx nx affected:e2e",
    "nx:affected:test": "npx nx affected:test",
    "nx:affected:lint": "npx nx affected:lint",
    "nx:affected:dep-graph": "npx nx affected:dep-graph",
    "nx:affected": "npx nx affected",
    "nx:format": "npx nx format:write",
    "nx:format:write": "npx nx format:write",
    "nx:format:check": "npx nx format:check",
    "nx:update": "npx nx migrate latest",
    "nx:graph": "npx nx dep-graph",
    "nx:workspace-schematic": "npx nx workspace-schematic",

    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "docker:ps": "docker-compose ps",
    "docker:restart": "docker-compose restart",
    
    "pulumi:dev": "cd infra/pulumi/dev && pulumi up",
    "pulumi:staging": "cd infra/pulumi/staging && pulumi up",
    "pulumi:prod": "cd infra/pulumi/prod && pulumi up",
    "pulumi:dev:destroy": "cd infra/pulumi/dev && pulumi destroy",
    
    "serverless:install": "cd infra/serverless/webhooks && npm install && cd ../etl && npm install && cd ../triggers && npm install",
    "serverless:deploy:webhooks": "cd infra/serverless/webhooks && serverless deploy",
    "serverless:deploy:etl": "cd infra/serverless/etl && serverless deploy",
    "serverless:deploy:triggers": "cd infra/serverless/triggers && serverless deploy",
    "serverless:deploy:all": "npm run serverless:deploy:webhooks && npm run serverless:deploy:etl && npm run serverless:deploy:triggers",
    "serverless:local:webhooks": "cd infra/serverless/webhooks && serverless offline",
    
    "infra:dev": "npm run docker:up && npm run pulumi:dev",
    "infra:local": "npm run docker:up"
  },
